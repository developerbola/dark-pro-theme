{
  "name": "dark-pro-vscode-default",
  "displayName": "Dark Pro (Github default & VScode default) Theme",
  "description": "Recolored 'Github Dark Default' & 'VSCode Dark Modern' color themes for VS Code",
  "publisher": "Mutawirr",
  "license": "MIT",
  "version": "0.3.1",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/developerbola/dark-pro-theme"
  },
  "bugs": {
    "url": "https://theme.com/developerbola/dark-pro-theme"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "github",
    "dark",
    "modern",
    "vs-code-modern",
    "github-default"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Dark Pro Github",
        "uiTheme": "vs-dark",
        "path": "./dark-pro-github/dark-pro-github.json"
      },
      {
        "label": "Dark Pro VSC",
        "uiTheme": "vs-dark",
        "path": "./dark-pro-vsc/dark-pro-vsc.json"
      },
      {
        "label": "Dark Pro Black & White",
        "uiTheme": "vs-dark",
        "path": "./dark-pro-b&w/dark-pro-b&w.json"
      },
      {
        "label": "Dark Pro Black & White VSC",
        "uiTheme": "vs-dark",
        "path": "./dark-pro-b&w-vsc/dark-pro-b&w-vsc.json"
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
